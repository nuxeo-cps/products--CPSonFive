<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="cmf_default">

  <!-- Define a default template with css-oriented slots
       skin is defined and applied in overrides, which is loaded first
  -->
  <browser:page
      for="Products.CMFCore.interfaces.IDynamicType"
      layer=".skin.ICPSDefaultSkin"
      template="five_template.pt"
      name="five_template"
      permission="zope.Public"
      />

</configure>
